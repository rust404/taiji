t step = () => {\n  if (n >= styleStr.length) return;\n  setTimeout(() => {\n    const char = readChar();\n    codeContainer.innerHTML += char;\n    styleContainer.innerHTML += char;\n    codePanel.scrollTo(0, 99999);\n    step();\n  }, 16);\n  function readChar() {\n    const char = styleStr[n++];\n    return char;\n  }\n};\nstep();\n"},"lineCount":null}},"error":null,"hash":"57e65c8c8b50a8effe1d6111a21f1993","cacheData":{"env":{}}}